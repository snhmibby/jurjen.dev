nav{overflow:hidden;background-color:#333;width:100%}nav ul{margin:0;padding:0;text-align:center;list-style-type:none}nav ul li{float:left;display:inline}nav ul li a{display:block;padding:5px;text-align:center;color:#fff;text-decoration:none}nav ul li a:hover{background-color:#111}nav ul li a.active{background-color:red}pre code{display:block;background:0 0;white-space:pre;-webkit-overflow-scrolling:touch;overflow-x:scroll;max-width:100%;min-width:100px;padding:0}code{font-size:10px}