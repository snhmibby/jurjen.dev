.nav-main{background-color:#333}.nav-main a{display:block;text-align:center;padding:5px;color:#fff;text-decoration:none}.nav-main a.active,.nav-main a:hover{background-color:darkred}.nav-main .nav-icon-bar,.nav-main .nav-menu{display:flex}@media(max-width:350px){.nav-main .nav-menu{flex-direction:column}}.nav-series{display:grid;grid-template-columns:2fr 1fr 2fr;text-align:center}.nav-series a{text-decoration:none}.nav-series .nav-prev{text-align:right}.nav-series .nav-next{text-align:left}.card-grid{display:grid}@media(min-width:450px){.card-grid{grid-template-columns:1fr 1fr}}@media(min-width:700px){.card-grid{grid-template-columns:1fr 1fr 1fr}}.summary{display:flex;flex-direction:column;justify-content:space-between;margin:3px;padding:10px;background:orange;border-radius:5px}.summary h1,.summary p{margin:1px}.summary a{margin-left:2px;margin-right:2px}.summary ul{margin:1px;display:flex;flex-direction:row;flex-wrap:wrap;justify-content:space-around;list-style:none}.summary:hover{background:teal}.collapsible{display:block;cursor:pointer;border-bottom:2px solid darkgrey}.collapsible::before{position:absolute;content:"-";left:10px}.collapsible.collapsible-closed::before{content:"+"}.collapsible-hide-h1,.collapsible-hide-h2,.collapsible-hide-h3{display:none!important}*{box-sizing:border-box}body{margin:0;padding:0;background:#ccc}hr{border:1px solid grey}h1,h2,h3,h4,h5{display:block;text-align:center;margin-top:10px}pre code{white-space:pre;overflow-x:scroll;display:block;max-width:100%;padding:5px}body>#content{position:relative;padding:10px}.nopadding{padding:0;margin:0}.toggle-visible{display:none!important}