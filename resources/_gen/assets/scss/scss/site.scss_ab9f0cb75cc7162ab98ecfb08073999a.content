.nav-main{background-color:#333}.nav-main a{display:block;text-align:center;padding:5px;color:#fff;text-decoration:none}.nav-main a.active,.nav-main a:hover{background-color:darkred}.nav-main .nav-icon-bar,.nav-main .nav-menu{display:flex}.nav-main .toggle-visible{display:none}@media(max-width:350px){.nav-main .nav-menu{flex-direction:column}}.nav-series{display:flex;justify-content:center;text-align:center}.nav-series a{text-decoration:none}.nav-series:first-child{display:flex;margin-left:auto;align-items:end}.nav-series:last-child{display:flex;margin-right:auto;align-items:end}.summary{margin:5px;padding:10px;background:lightgrey;border-radius:15px}.summary h1,.summary p{margin:1px}.summary a{text-decoration:none}.summary ul{margin:1px;display:flex;flex-direction:row;flex-wrap:wrap;justify-content:space-around;list-style:none}.summary:hover{background:darkgrey}*{box-sizing:border-box}body{margin:0;background:#f1f1f1}hr{border:1px solid grey}#content{padding:10px}code{font-size:10px}.card-grid{display:flex;flex-wrap:wrap;justify-content:space-around}.card-grid section{max-width:90%}@media(min-width:350px){.card-grid section{max-width:45%}}@media(min-width:700px){.card-grid section{max-width:30%}}